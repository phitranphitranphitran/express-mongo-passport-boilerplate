{
  "name": "express-mongo-passport-boilerplate",
  "description": "Boilerplate for full-stack Node.js apps",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:dev": "concurrently --kill-others \"gulp watch\" \"node-inspector ./bin/www\" \"nodemon --debug ./bin/www\"",
    "start:prod": "node ./bin/www",
    "build:prod": "gulp",
    "postinstall": "npm run build:prod",
    "debug": "node-debug ./bin/www",
    "test": "set NODE_ENV=test&&mocha --recursive",
    "test:watch": "npm test -- --watch",
    "lint": "eslint ."
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "connect-mongo": "^1.1.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-flash": "^0.0.2",
    "express-session": "^1.13.0",
    "express-validator": "^2.20.4",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.1",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^1.6.0",
    "jade": "^1.11.0",
    "lusca": "^1.4.1",
    "mongoose": "^4.4.17",
    "morgan": "^1.7.0",
    "passport": "0.3.2",
    "passport-facebook": "^2.1.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4"
  },
  "devDependencies": {
    "async": "^2.0.0-rc.6",
    "chai": "^3.5.0",
    "concurrently": "^2.1.0",
    "eslint": "^2.11.1",
    "gulp-util": "^3.0.7",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0"
  },
  "engines": {
    "node": "6.1.0",
    "npm": "2.14.12"
  },
  "license": "MIT"
}
